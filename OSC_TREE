      /rw (0 / 1 / 2)
      /inv (0 / 1 / 2)
      /stop (0 / 1 / 2)
      /loop (0 / 1 / 2)
      /loopMode (0 / 1 / 2)
      /audio (0 / 1 / 2)
      /samplerate (0 / 1 / 2)
      /boost (0 / 1 / 2)
      /avAmp (0 / 1 / 2)
      /avLop (0 / 1 / 2)
      /avHip (0 / 1 / 2)
