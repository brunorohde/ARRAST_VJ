/output
  /dimen (0 / 1 / 2 / 3)   * 320x240 / 640x480 / 1024x768 / 1280x720
  /dimX (int)
  /dimY (int)
  /master (0 / 1)
  /preview (0 / 1)
  /fps (int)
  /cursor (0 / 1)
  /border (0 / 1)
  /redBg (0 - 1)
  /greenBg (0 - 1)
  /blueBg (0 - 1)
  /offsetX (int)
  /offsetY (int)
  /debug (0 / 1)
  /restart
  /cameraDev (int)
  /preset (0 / 1)
